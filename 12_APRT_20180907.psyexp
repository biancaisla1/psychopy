      <CodeComponent name="trials">
        <Param name="Begin Experiment" updates="constant" val="#set experiment-wide levels of reward&amp;#10;lowPointMin=5&amp;#10;lowPointMax=25&amp;#10;highPointMin=50&amp;#10;highPointMax=250&amp;#10;&amp;#10;#set experiment-wide levels of injury&amp;#10;lowInjuryMin = 5&amp;#10;lowInjuryMax = 35&amp;#10;highInjuryMin = 60&amp;#10;highInjuryMax = 90&amp;#10;&amp;#10;#set experiment-wide maximum health&amp;#10;maxHealth=100&amp;#10;&amp;#10;&amp;#10;#Initialize trial-level variables&amp;#10;trialCounter=0 #first picture trial=1&amp;#10;trialInjuriesCounter=0 #reset per trial&amp;#10;trialRewardsCounter=0 #reset per trial&amp;#10;pPressCounter=0 #reset per trial&amp;#10;deathsCounter=0 &amp;#10;pSinceInjuryCounter=0 #reset per injury (not trial-this can accumulate across trials!)&amp;#10;pSinceRewardCounter=0 #reset per reward (not trial- this can accumulate across trials!)&amp;#10;totalPoints=0" valType="extendedCode"/>
        <Param name="Begin JS Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="Begin JS Routine" updates="constant" val="" valType="extendedCode"/>
        <Param name="Begin Routine" updates="constant" val="#Reset variables&amp;#10;health=maxHealth&amp;#10;pts=0&amp;#10;pPressCounter=0&amp;#10;trialInjuriesCounter=0&amp;#10;trialRewardsCounter=0&amp;#10;trialCounter=trialCounter+1&amp;#10;&amp;#10;#Initialize display variables&amp;#10;statusDisplay=(description)&amp;#10;&amp;#10;#Flush all key responses made during delay&amp;#10;event.clearEvents()&amp;#10;&amp;#10;#set background colors&amp;#10;#solid colors (risk level)&amp;#10;if (injuryProbability)==5:&amp;#10;    solidColor='yellow'&amp;#10;if (injuryProbability)==15:&amp;#10;    solidColor='blue'&amp;#10;#stripe colors (reward level)&amp;#10;if (rewardProbability)==10:&amp;#10;    stripeColor='white'&amp;#10;if (rewardProbability)==3:&amp;#10;    stripeColor=solidColor" valType="extendedCode"/>
        <Param name="Code Type" updates="None" val="Auto-&gt;JS" valType="str"/>
        <Param name="Each Frame" updates="constant" val="# if proceed&amp;#10;if &quot;p&quot; in theseKeys:&amp;#10;    injuryChooser=randint(1,(injuryProbability))&amp;#10;    rewardChooser=randint(1,(rewardProbability))&amp;#10;    &amp;#10;    pPressCounter=pPressCounter+1&amp;#10;    &amp;#10;    #injury&amp;#10;    if injuryChooser==1:&amp;#10;        pSinceInjuryCounter=0&amp;#10;        trialInjuriesCounter=trialInjuriesCounter+1&amp;#10;        &amp;#10;        &amp;#10;        #Set amount of injury&amp;#10;        if (danger)==4:&amp;#10;            healthPenalty= maxHealth&amp;#10;        if (danger)==3:&amp;#10;            healthPenalty= randint(highInjuryMin, highInjuryMax)&amp;#10;        if (danger)==1:&amp;#10;            healthPenalty= randint(lowInjuryMin, lowInjuryMax)&amp;#10;        &amp;#10;        #Update Display&amp;#10;        health=health-healthPenalty&amp;#10;        pts=int(round(pts*((1-(healthPenalty/maxHealth)/2)))) # Points multiplied by a fraction of the health penalty to reduce points&amp;#10;        statusDisplay=&quot;OH NO! You have been injured! Choose again.&quot;&amp;#10;        &amp;#10;        #cap points at 0&amp;#10;        if pts&lt;=0:&amp;#10;            pts=0&amp;#10;        &amp;#10;        #Terminate trial if death&amp;#10;        if health&lt;=0:&amp;#10;            # Set trial to end&amp;#10;            loopTrial.finished=True&amp;#10;            continueRoutine=False&amp;#10;            showDeath=1&amp;#10;            # Set points and health to 0; increment death counter&amp;#10;            pts=0&amp;#10;            health=0&amp;#10;            deathsCounter=deathsCounter+1&amp;#10;    &amp;#10;    #no injury&amp;#10;    if injuryChooser&gt;1:&amp;#10;        pSinceInjuryCounter=pSinceInjuryCounter+1&amp;#10;        &amp;#10;        #reward&amp;#10;        if rewardChooser&gt;1:&amp;#10;            &amp;#10;            trialRewardsCounter=trialRewardsCounter+1&amp;#10;            pSinceRewardCounter=pSinceRewardCounter+1&amp;#10;            &amp;#10;            if (points)==1:&amp;#10;                pts=pts+randint(lowPointMin,lowPointMax)&amp;#10;                statusDisplay=&quot;Lucky you - points awarded!&quot;&amp;#10;            if (points)==3:&amp;#10;                pts=pts+randint(highPointMin,highPointMax)&amp;#10;                statusDisplay=&quot;Lucky you - points awarded!&quot;&amp;#10;        &amp;#10;        #no reward&amp;#10;        if rewardChooser==1:&amp;#10;            pts=pts&amp;#10;            statusDisplay=&quot;Sorry - no points awarded.&quot;&amp;#10;            pSinceRewardCounter=0&amp;#10;    &amp;#10;    #Finish off key press&amp;#10;    loopTrial.finished=True&amp;#10;&amp;#10;#if quit &amp;#10;if &quot;q&quot; in theseKeys:&amp;#10;    loopTrial.finished=True&amp;#10;    continueRoutine=False&amp;#10;    showDeath=0&amp;#10;    trialsSinceInjuryCounter=trialsSinceInjuryCounter+1&amp;#10;    totalPoints=totalPoints+pts" valType="extendedCode"/>
        <Param name="Each JS Frame" updates="constant" val="" valType="extendedCode"/>
        <Param name="End Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="End JS Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="End JS Routine" updates="constant" val="" valType="extendedCode"/>
        <Param name="End Routine" updates="constant" val="#make sure that points update before the next trial starts&amp;#10;" valType="extendedCode"/>
        <Param name="disabled" updates="None" val="False" valType="bool"/>
        <Param name="name" updates="None" val="trials" valType="code"/>
      </CodeComponent>
      <CodeComponent name="writeFile">
        <Param name="Begin Experiment" updates="constant" val="# Initialize file names and writing: CHANGE TO MATCH DIRECTORY IN DATA FILENAME!&amp;#10;filedir = &quot;../data/&quot;&amp;#10;filename = '%s%s_subject-%s_session-%s_date-%s.txt' %(filedir,expName, expInfo['participant'], expInfo['session'], expInfo['date'])&amp;#10;file = open(filename, &quot;a&quot;)&amp;#10;filenameTrials = '%s%s-trials_subject-%s_session-%s_date-%s.txt' %(filedir,expName, expInfo['participant'], expInfo['session'], expInfo['date'])&amp;#10;fileTrials = open(filenameTrials, &quot;a&quot;)" valType="extendedCode"/>

        <Param name="Code Type" updates="None" val="Auto-&gt;JS" valType="str"/>
        <Param name="Each Frame" updates="constant" val="if &quot;p&quot; in theseKeys:&amp;#10;    file.write(expInfo['date'] + &quot;\t&quot; + expInfo['participant'] + &quot;\t&quot; + expInfo['session'] + &quot;\t&quot; + str(trialCounter) + &quot;\t&quot; + str(PicTypeNum) + &quot;\t&quot; + str(danger) + &quot;\t&quot; + str(points) + &quot;\t&quot; + str(injuryProbability) + &quot;\t&quot; + str(rewardProbability) + &quot;\t&quot; + str(health) + &quot;\t&quot; + str(pts) + &quot;\t&quot; + str(trialInjuriesCounter) + &quot;\t&quot; + str(trialRewardsCounter) + &quot;\t&quot; + str(pPressCounter) + &quot;\t&quot; + str(pSinceRewardCounter) + &quot;\t&quot; + str(deathsCounter) + &quot;\t&quot; + str(pSinceInjuryCounter) + &quot;\t&quot; + str(totalPoints) + &quot;\t&quot; + str(theseKeys) + &quot;\n&quot;)&amp;#10;&amp;#10;if &quot;q&quot; in theseKeys:&amp;#10;    file.write(expInfo['date'] + &quot;\t&quot; + expInfo['participant'] + &quot;\t&quot; + expInfo['session'] + &quot;\t&quot; + str(trialCounter) + &quot;\t&quot; + str(PicTypeNum) + &quot;\t&quot; + str(danger) + &quot;\t&quot; + str(points) + &quot;\t&quot; + str(injuryProbability) + &quot;\t&quot; + str(rewardProbability) + &quot;\t&quot; + str(health) + &quot;\t&quot; + str(pts) + &quot;\t&quot; + str(trialInjuriesCounter) + &quot;\t&quot; + str(trialRewardsCounter) + &quot;\t&quot; + str(pPressCounter) + &quot;\t&quot; + str(pSinceRewardCounter) + &quot;\t&quot; + str(deathsCounter) + &quot;\t&quot; + str(pSinceInjuryCounter) + &quot;\t&quot; + str(totalPoints) + &quot;\t&quot; + str(theseKeys) + &quot;\n&quot;)&amp;#10;" valType="extendedCode"/>
        <Param name="Each JS Frame" updates="constant" val="" valType="extendedCode"/>
        <Param name="End Experiment" updates="constant" val="file.close()" valType="extendedCode"/>
        <Param name="End JS Experiment" updates="constant" val="" valType="extendedCode"/>
